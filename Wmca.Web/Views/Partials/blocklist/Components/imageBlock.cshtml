@inherits UmbracoViewPage<Umbraco.Cms.Core.Models.Blocks.BlockListItem>
@using ContentModels = Wmca.Models.Content;
@using Umbraco.Cms.Core.Models

@{
    var content = (ContentModels.ImageBlock)Model.Content;
}

@{
    var typedMediaPickerSingle = content.Value<MediaWithCrops>("image");
    if (typedMediaPickerSingle != null)
    {
        <img src="@typedMediaPickerSingle.MediaUrl()" style="width:200px" alt="@typedMediaPickerSingle.Value("altText", fallback: Fallback.ToDefaultValue, defaultValue: @typedMediaPickerSingle.Name)" />
    }
}